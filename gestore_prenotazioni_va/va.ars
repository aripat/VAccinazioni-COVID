	CONDITION
		CALL is_request_method WITH "POST"
		http.url is in msvc_prenotazione
		exists http.data["CF"]
		exists http.data["categoria"]
		exists http.data["TEAM"]
		//dati prenotazione
        exists http.cookie["validation"]
        CALL is_valid_cookie WITH http.cookie["validation"] 
	ACTION
